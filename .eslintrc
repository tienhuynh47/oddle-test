{
    "parser": "babel-eslint",
    "rules": {
        "indent": ["error", 4],
        "max-len": [1, 200],
        "comma-dangle": [0, "never"],
        "func-names": 0,
        "no-param-reassign": 0,
        "curly": [2, "all"],
        "no-trailing-spaces": 0,
        "prefer-template": 0,
        "no-restricted-properties": 0,
        "linebreak-style": 0,
    },
    "env": {
        "browser": true,
        "es6": true
    }
}
